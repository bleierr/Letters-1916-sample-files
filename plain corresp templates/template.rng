<?xml version="1.0" encoding="UTF-8"?>
<grammar ns="http://www.tei-c.org/ns/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="TEI">
      <element name="teiHeader">
        <attribute>
          <name ns="http://www.w3.org/XML/1998/namespace">id</name>
          <data type="NCName"/>
        </attribute>
        <element name="fileDesc">
          <element name="titleStmt">
            <element name="title">
              <text/>
            </element>
            <element name="author">
              <text/>
            </element>
          </element>
          <element name="editionStmt">
            <element name="edition">
              <text/>
            </element>
            <oneOrMore><element name="respStmt">
              <element name="resp">
                <text/>
              </element>
              <ref name="name"/>
            </element></oneOrMore>
          </element>
          <element name="publicationStmt">
            <element name="publisher">
              <ref name="address"/>
            </element>
            <ref name="idno"/>
            <element name="availability">
              <attribute name="status">
                <data type="NCName"/>
              </attribute>
              <oneOrMore>
                <ref name="p"/>
              </oneOrMore>
            </element>
            <ref name="date"/>
          </element>
          <element name="notesStmt">
            <ref name="note"/>
            <optional><ref name="note"/></optional>
            
          </element>
          <element name="sourceDesc">
            <element name="msDesc">
              <element name="msIdentifier">
                <element name="repository">
                  <attribute name="ref">
                      <choice>
                        <value>#AL</value>
                        <value>#CCCA</value>
                        <value>#ICPA</value>
                        <value>#IJA</value>
                        <value>#LDA</value>
                        <value>#MMM</value>
                        <value>#MAI</value>
                        <value>#NAI</value>
                        <value>#NLI</value>
                        <value>#NMI</value>
                        <value>#PRONI</value>
                        <value>#PCMA</value>
                        <value>#TCD</value>
                        <value>#UCD</value>
                        <value>#AIHS</value>
                        <value>#BlairHalliday</value>
                        <value>#FinnFamily</value>
                        <value>#LawrenceScott</value>
                        <value>#LiamSmith</value>
                        <value>#Private_Collection</value>
                        <value>#MichaelHannah</value>
                        <value>#PatCarroll</value>
                        <value>#RoryOLoughlin</value>
                        <value>#JamesRyle</value>
                        <value>#SighleHumphreys</value>
                        <value>#PatEnglish</value>
                        <value>#ONeillFamily</value>
                        <value>#OLaoghaireFamily</value>
                        <value>#McGrath</value>
                        <value>#KellyFamily</value>
                        <value>#Garland</value>
                        <value>#OModhrainFamily</value>
                      </choice>
                  </attribute>
                  <text/>
                </element>
                <element name="collection">
                  <attribute name="type">
                    <choice>
                      <value>public</value>
                      <value>private</value>
                    </choice>
                  </attribute>
                  <text/>
                </element>
                <ref name="idno"/>
              </element>
            </element>
          </element>
        </element>
        <element name="profileDesc">
          <element name="correspDesc">
            <attribute name="xml:id">
              <ref name="correspID"></ref>
            </attribute>
            <oneOrMore>
              <element name="correspAction">
                <attribute name="type">
                  <choice>
                    <value>received</value>
                    <value>sent</value>
                  </choice>
                </attribute>
                <zeroOrMore>
                  <ref name="persName"></ref>
                  </zeroOrMore>
                <optional>
                  <ref name="placeName"/>
                </optional>
                <optional>
                  <ref name="date"/>
                </optional>
              </element>
            </oneOrMore>
          </element>
          <element name="textClass">
            <element name="keywords">
              <element name="list">
                <oneOrMore>
                  <element name="item">
                    <attribute name="n">
                      <choice>
                        <value>gender</value>
                        <value>tag</value>
                        <value>death</value>
                      </choice>
                    </attribute>
                    <text/>
                  </element>
                </oneOrMore>
              </element>
            </element>
          </element>
          <element name="langUsage">
            <element name="language">
              <attribute name="ident">
                <choice>
                  <value>en</value>
                  <value>ga</value>
                </choice>
              </attribute>
              <choice>
                <value>English</value>
                <value>Irish</value>
              </choice>
            </element>
          </element>
        </element>
        <element name="encodingDesc">
          <element name="editorialDecl">
            <ref name="p"/>
          </element>
          <element name="projectDesc">
            <ref name="p"/>
          </element>
          <element name="appInfo">
            <element name="application">
              <attribute name="ident">
                <data type="NCName"/>
              </attribute>
              <attribute name="version">
                <data type="integer"/>
              </attribute>
              <attribute>
                <name ns="http://www.w3.org/XML/1998/namespace">id</name>
                <data type="NCName"/>
              </attribute>
              <element name="desc">
                <text/>
              </element>
            </element>
          </element>
        </element>
        <element name="revisionDesc">
          <oneOrMore>
            <element name="change">
              <attribute name="when">
                <data type="dateTime"/>
              </attribute>
              <attribute name="who"/>
              <text/>
            </element>
          </oneOrMore>
        </element>
      </element>
      <element name="facsimile">
        <oneOrMore><ref name="graphic"></ref></oneOrMore>
      </element>
      <ref name="text"/>
    </element>
  </start>
  <define name="name">
    <element name="name">
      <text/>
    </element>
  </define>
  <define name="address">
    <element name="address">
      <choice>
        <oneOrMore>
          <element name="addrLine">
            <oneOrMore>
              <choice>
                <text/>
                <ref name="date"/>
                <ref name="foreign"/>
                <ref name="lineLevel"></ref>
              </choice>
            </oneOrMore>
          </element>
        </oneOrMore>
        <group>
          <ref name="name"/>
          <element name="orgName">
            <text/>
          </element>
          <element name="region">
            <text/>
          </element>
          <ref name="placeName"/>
        </group>
      </choice>
    </element>
  </define>
  <define name="idno">
    <element name="idno">
      <optional>
        <attribute name="n">
          <data type="NCName"/>
        </attribute>
      </optional>
      <text/>
    </element>
  </define>
  <define name="p">
    <element name="p">
      <oneOrMore>
        <choice>
          <text/>
          <ref name="breaks"/>
          <ref name="lineLevel"></ref>
          <ref name="foreign"></ref>
          </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="date">
    <element name="date">
      <optional>
        <attribute name="when">
          <ref name="dateValue"></ref>
        </attribute>
      </optional>
      <oneOrMore>
        <choice>
          <text/>
          <ref name="hi"/>
          <ref name="add"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="note">
    <element name="note">
      <optional>
        <attribute name="type">
          <choice>
            <value>summary</value>
            <value>translation</value>
          </choice>
        </attribute>
      </optional>
      <oneOrMore>
        <choice>
          <text/>
          <ref name="breaks"/>
          <ref name="p"/>
          <ref name="address"></ref>
          <ref name="lineLevel"></ref>
        </choice>
      </oneOrMore>
    </element>
  </define>
